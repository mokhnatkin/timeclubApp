{
  "name": "clients",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "arrivalTime": {
      "type": "date",
      "required": true
    },
    "checkoutTime": {
      "type": "date"
    },
    "amount": {
      "type": "number"
    },
    "isFree": {
      "type": "boolean",
      "default": false
    },
    "promotion": {
      "type": "string"
    },
    "comment": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "isChecked": {
      "type": "boolean",
      "default": false
    },
    "timeInClub": {
      "type": "number"
    },
    "isEmployee": {
      "type": "boolean",
      "default": false
    },
    "emplID": {
      "type": "string"
    },
    "isEmployeeAtWork": {
      "type": "boolean",
      "default": false
    },
    "companyN": {
      "type": "number"
    },
    "isDirector": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
