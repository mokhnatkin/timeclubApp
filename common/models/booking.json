{
  "name": "booking",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "date": {
      "type": "date",
      "required": true
    },
    "numberOfPeople": {
      "type": "number"
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "place": {
      "type": "string"
    },
    "isBirthday": {
      "type": "boolean",
      "default": false
    },
    "prePayment": {
      "type": "number"
    },
    "isSelected": {
      "type": "boolean",
      "default": false
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "Новая"
    },
    "comment": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
